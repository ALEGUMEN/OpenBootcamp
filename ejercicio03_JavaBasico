package com.company;

public class bucle {

    public static void main(String[] args) {

        //Array de tipo String
        String[] nombres = {"Pedro", "Damian", "Alejandro"};

        //recorremos el array y mostramos su contenido
        for(int i = 0; i < nombres.length; i++) {
            System.out.println(nombres[i]);
        }

    }

}
